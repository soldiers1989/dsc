package com.yixin.web.dto.monitor;

/**
 * 申请单各阶段时长统计信息
 *
 * @description:
 * @date: 2018-10-15 14:05
 */
public class OrderStepDurationDto {

    /**
     * 所在状态
     */
    private Integer status;
    /**
     * 最短时长
     */
    private Long min;
    /**
     * 最长时长
     */
    private Long max;
    /**
     * 中间时长
     */
    private Long mid;
    /**
     * 平均时长
     */
    private Long average;


    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }

    public Long getMin() {
        return min;
    }

    public void setMin(Long min) {
        this.min = min;
    }

    public Long getMax() {
        return max;
    }

    public void setMax(Long max) {
        this.max = max;
    }

    public Long getMid() {
        return mid;
    }

    public void setMid(Long mid) {
        this.mid = mid;
    }

    public Long getAverage() {
        return average;
    }

    public void setAverage(Long average) {
        this.average = average;
    }
}
