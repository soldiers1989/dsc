import com.yixin.common.utils.BaseDTO;
import com.yixin.kepler.common.enums.BankPhaseEnum;

import java.io.Serializable;

/**
 * Package : com.yixin.kepler.dto
 *      异步交易DTO
 * @author YixinCapital -- liushuai2
 *         2018年06月10日 12:16
 */
public class AsyncTranDTO extends BaseDTO {

    String applyNo;

    BankPhaseEnum phaseEnum;

    public AsyncTranDTO(){

    }

    public AsyncTranDTO(String applyNo, BankPhaseEnum phaseEnum){
        this.applyNo = applyNo;
        this.phaseEnum = phaseEnum;
    }
    public String getApplyNo() {
        return applyNo;
    }

    public void setApplyNo(String applyNo) {
        this.applyNo = applyNo;
    }

    public BankPhaseEnum getPhaseEnum() {
        return phaseEnum;
    }

    public void setPhaseEnum(BankPhaseEnum phaseEnum) {
        this.phaseEnum = phaseEnum;
    }
}
