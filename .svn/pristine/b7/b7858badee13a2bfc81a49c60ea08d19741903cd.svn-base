package com.yixin.kepler.v1.datapackage.dto.yntrust;

import com.fasterxml.jackson.annotation.JsonProperty;

import java.math.BigDecimal;

/**
 * @author YixinCapital -- chenjiacheng
 *         2018年09月19日 11:24
 **/

public class YTCreateOrderPaymentBankCardsDto {

	/**
	 * 放款银行账号
	 */
	@JsonProperty("AccountNo")
	private String accountNo;

	/**
	 * 银行编码
	 */
	@JsonProperty("BankCode")
	private String bankCode;

	/**
	 * 支行名称
	 */
	@JsonProperty("BranchBankName")
	private String branchBankName;

	/**
	 * 联行号
	 */
	@JsonProperty("stdBankCode")
	private String StdBankCode;

	/**
	 * 银行卡归属地
	 */
	@JsonProperty("bankCardAttribution")
	private String BankCardAttribution;

	/**
	 * 账户开户名
	 */
	@JsonProperty("accountName")
	private String AccountName;

	/**
	 * 放款金额
	 */
	@JsonProperty("amount")
	private BigDecimal Amount;

	/**
	 * 放款对象类型
	 */
	@JsonProperty("type")
	private String Type;


	public String getAccountNo() {
		return accountNo;
	}

	public void setAccountNo(String accountNo) {
		this.accountNo = accountNo;
	}

	public String getBankCode() {
		return bankCode;
	}

	public void setBankCode(String bankCode) {
		this.bankCode = bankCode;
	}

	public String getBranchBankName() {
		return branchBankName;
	}

	public void setBranchBankName(String branchBankName) {
		this.branchBankName = branchBankName;
	}

	public String getStdBankCode() {
		return StdBankCode;
	}

	public void setStdBankCode(String stdBankCode) {
		StdBankCode = stdBankCode;
	}

	public String getBankCardAttribution() {
		return BankCardAttribution;
	}

	public void setBankCardAttribution(String bankCardAttribution) {
		BankCardAttribution = bankCardAttribution;
	}

	public String getAccountName() {
		return AccountName;
	}

	public void setAccountName(String accountName) {
		AccountName = accountName;
	}

	public BigDecimal getAmount() {
		return Amount;
	}

	public void setAmount(BigDecimal amount) {
		Amount = amount;
	}

	public String getType() {
		return Type;
	}

	public void setType(String type) {
		Type = type;
	}
}
