package com.yixin.kepler.dto;

import java.io.Serializable;
import java.util.List;


public class RespFinancialDTO implements Serializable{

	private static final long serialVersionUID = 1L;
	
	private List<RespFinancialAction> action;
	
	private List<RespFinancialColumn> columns;
	
	private List<RespFinancialFile>  files;
	
	/**
	 * 阶段
	 */
	private String phase;

	public List<RespFinancialAction> getAction() {
		return action;
	}

	public void setAction(List<RespFinancialAction> action) {
		this.action = action;
	}

	public List<RespFinancialColumn> getColumns() {
		return columns;
	}

	public void setColumns(List<RespFinancialColumn> columns) {
		this.columns = columns;
	}

	public List<RespFinancialFile> getFiles() {
		return files;
	}

	public void setFiles(List<RespFinancialFile> files) {
		this.files = files;
	}

	public String getPhase() {
		return phase;
	}

	public void setPhase(String phase) {
		this.phase = phase;
	}
	
	
	

}
