package com.yixin.kepler.v1.datapackage.dto.other;

import java.io.Serializable;
import java.math.BigDecimal;
import java.util.Date;

/**
 * 放款信息
 * Package : com.yixin.kepler.v1.datapackage.dto.other
 * 
 * @author YixinCapital -- wangwenlong
 *		   2018年10月15日 下午6:19:18
 *
 */
public class LoanInfoDTO implements Serializable {

	private static final long serialVersionUID = 8709740176852117706L;
	
	/**
	 * 申请编号
	 */
	private String applyNo;
	/**
	 * 银行流水号
	 */
	private String bankSeq;
	/**
	 * 放款日期yyyyMMdd
	 */
	private Date loanDime;
	/**
	 * 放款金额
	 */
	private BigDecimal loanAmount;
	
	/**
	 * 放款状态
	 */
	private Boolean loanSuccess;
	
	/**
	 * 资方编码
	 */
	private String financialCode;
	
	/**
	 * 云信放款状态
	 */
	private String processStatus;
	
	
	public String getProcessStatus() {
		return processStatus;
	}
	public void setProcessStatus(String processStatus) {
		this.processStatus = processStatus;
	}
	public String getFinancialCode() {
		return financialCode;
	}
	public void setFinancialCode(String financialCode) {
		this.financialCode = financialCode;
	}
	public String getApplyNo() {
		return applyNo;
	}
	public void setApplyNo(String applyNo) {
		this.applyNo = applyNo;
	}
	public String getBankSeq() {
		return bankSeq;
	}
	public void setBankSeq(String bankSeq) {
		this.bankSeq = bankSeq;
	}
	public Date getLoanDime() {
		return loanDime;
	}
	public void setLoanDime(Date loanDime) {
		this.loanDime = loanDime;
	}
	public BigDecimal getLoanAmount() {
		return loanAmount;
	}
	public void setLoanAmount(BigDecimal loanAmount) {
		this.loanAmount = loanAmount;
	}
	public Boolean getLoanSuccess() {
		return loanSuccess;
	}
	public void setLoanSuccess(Boolean loanSuccess) {
		this.loanSuccess = loanSuccess;
	}
}
