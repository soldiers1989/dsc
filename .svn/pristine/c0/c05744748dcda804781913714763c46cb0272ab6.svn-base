public class YTCreateOrderMergencyContactDto {

	/**
	 * 与借款人关系
	 */
	private String MergencyContactRelation;

	/**
	 * 紧急联系人姓名
	 */
	private String MergencyContactName;

	/**
	 * 联系电话
	 */
	private String MergencyContactPhone;

	public String getMergencyContactRelation() {
		return MergencyContactRelation;
	}

	public void setMergencyContactRelation(String mergencyContactRelation) {
		MergencyContactRelation = mergencyContactRelation;
	}

	public String getMergencyContactName() {
		return MergencyContactName;
	}

	public void setMergencyContactName(String mergencyContactName) {
		MergencyContactName = mergencyContactName;
	}

	public String getMergencyContactPhone() {
		return MergencyContactPhone;
	}

	public void setMergencyContactPhone(String mergencyContactPhone) {
		MergencyContactPhone = mergencyContactPhone;
	}
}
