package com.yixin.kepler.dto.webank;

import java.io.Serializable;

import com.fasterxml.jackson.annotation.JsonProperty;

/**
 * 首付成分
 * Package : com.yixin.kepler.dto.webank
 * 
 * @author YixinCapital -- wangwenlong
 *		   2018年7月9日 下午8:46:07
 *
 */
public class WBDownPaymentDTO implements Serializable {
	
	private static final long serialVersionUID = -8527152941691121075L;

	/**
	 * 首付成分名称
	 */
	@JsonProperty("COMPONENT_NAME")
	private String componentName;

	/**
	 * 首付成分金额
	 */
	@JsonProperty("COMPONENT_AMOUNT")
	private String componentAmount;
	
	/**
	 * 首付比例
	 */
	@JsonProperty("DOWN_PAYMENT_RATIO")
	private String downPaymentRatio;

	public String getComponentName() {
		return componentName;
	}

	public void setComponentName(String componentName) {
		this.componentName = componentName;
	}

	public String getComponentAmount() {
		return componentAmount;
	}

	public void setComponentAmount(String componentAmount) {
		this.componentAmount = componentAmount;
	}

	public String getDownPaymentRatio() {
		return downPaymentRatio;
	}

	public void setDownPaymentRatio(String downPaymentRatio) {
		this.downPaymentRatio = downPaymentRatio;
	}
	
}
